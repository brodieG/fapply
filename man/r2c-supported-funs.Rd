\itemize{
\item Arithmetic: \code{+}, \code{-}, \code{*}, \code{/}, and \code{^}.
\item Relational: \code{<}, \code{<=}, \code{>}, \code{>=}, \code{==}, \code{!=}.
\item Logical: \code{&}, \code{&&}, \code{|}, \code{||}, \code{!}, \code{ifelse}.
\itemize{
\item \code{&&} and \code{||} always evaluate all parameters.
\item \code{ifelse} return in a type that can support both \code{yes} and \code{no} values.
}
\item Statistics: \code{mean}, \code{sum}, \code{length}, \code{all}, \code{any}.
\itemize{
